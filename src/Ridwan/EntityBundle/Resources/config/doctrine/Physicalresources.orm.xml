<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ridwan\EntityBundle\Entity\Physicalresources" table="PhysicalResources">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="smallint" column="type" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="causes" type="string" column="causes" length="45" nullable="true"/>
    <field name="organizations" type="text" column="organizations" nullable="true"/>
  </entity>
</doctrine-mapping>
