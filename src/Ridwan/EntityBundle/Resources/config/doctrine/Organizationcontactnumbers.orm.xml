<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ridwan\EntityBundle\Entity\Organizationcontactnumbers" table="OrganizationContactNumbers">
    <unique-constraints>
      <unique-constraint name="number_UNIQUE" columns="number"/>
      <unique-constraint name="User_UNIQUE" columns="User"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="number" type="smallint" column="number" nullable="false"/>
    <field name="type" type="smallint" column="type" nullable="false"/>
    <many-to-one field="user" target-entity="Organizationcontactdetails">
      <join-columns>
        <join-column name="User" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
