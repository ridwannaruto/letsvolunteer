<?php

namespace Ridwan\EntityBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * OrganizationContactNumbers
 */
class OrganizationContactNumbers
{
    /**
     * @var integer
     */
    private $number;

    /**
     * @var integer
     */
    private $type;

    /**
     * @var integer
     */
    private $id;

    /**
     * @var \Ridwan\EntityBundle\Entity\OrganizationContactDetails
     */
    private $user;


    /**
     * Set number
     *
     * @param integer $number
     * @return OrganizationContactNumbers
     */
    public function setNumber($number)
    {
        $this->number = $number;

        return $this;
    }

    /**
     * Get number
     *
     * @return integer 
     */
    public function getNumber()
    {
        return $this->number;
    }

    /**
     * Set type
     *
     * @param integer $type
     * @return OrganizationContactNumbers
     */
    public function setType($type)
    {
        $this->type = $type;

        return $this;
    }

    /**
     * Get type
     *
     * @return integer 
     */
    public function getType()
    {
        return $this->type;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set user
     *
     * @param \Ridwan\EntityBundle\Entity\OrganizationContactDetails $user
     * @return OrganizationContactNumbers
     */
    public function setUser(\Ridwan\EntityBundle\Entity\OrganizationContactDetails $user = null)
    {
        $this->user = $user;

        return $this;
    }

    /**
     * Get user
     *
     * @return \Ridwan\EntityBundle\Entity\OrganizationContactDetails 
     */
    public function getUser()
    {
        return $this->user;
    }
}
